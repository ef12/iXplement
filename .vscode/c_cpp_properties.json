{
  "configurations": [
    {
      "name": "windows-gcc-x64",
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "compilerPath": "gcc",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "windows-gcc-x64",
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UICODE",
        "USE_HAL_DRIVER",
        "STM32F429xx"
      ],
      "compilerArgs": [
        "-mcpu=cortex-m4",
        "-mthumb"
      ]
    }
  ],
  "version": 4
}